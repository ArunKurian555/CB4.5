#ENABLE_STACK_CHECKING
#ENABLE_TRACE     
#SYMBOL_NAME "Serial Selector 2.2"
#DEFAULT_VOLATILE

#help_begin
Filter analog input based on digital input after .5sec
#help_end

/**** I/O Variables ****/
string_input SerialI[400][400];
string_output SerialO[400];         
DIGITAL_INPUT Trigger;
DIGITAL_INPUT Convert[400];
integer i;
/**** Functions ****/
Push Trigger
{
for(i=1 to 400)
{
if(Convert[i]=1)
{
SerialO[i]=SerialI[i];
}
}
}
